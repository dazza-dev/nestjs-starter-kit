model Module {
  id        Int      @id @default(autoincrement())
  name      String
  slug      String   @unique
  icon      String?
  order     Int      @default(0)
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  permissions Permission[]

  @@map("modules")
}
